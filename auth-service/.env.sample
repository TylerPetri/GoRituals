# HS256
JWT_ALG=HS256
JWT_ISSUER=https://auth.example.com
JWT_AUDIENCE=my-api
JWT_HS256_KID=v1
JWT_HS256_SECRET_B64=<base64url-of-secret>
PUBLIC_BASE_URL=https://auth.example.com
HTTP_ADDR=:8080

# RS256
JWT_ALG=RS256
JWT_ISSUER=https://auth.example.com
JWT_AUDIENCE=my-api
JWT_RS256_ACTIVE_KID=kid-2025-10
JWT_RS256_PRIVATE_KEY_FILE=/secrets/rs256.key
JWT_RS256_PUBLIC_KEYS=kid-2025-06=/secrets/old.pub
PUBLIC_BASE_URL=https://auth.example.com
HTTP_ADDR=:8080

# ED25519
JWT_ALG=EdDSA
JWT_ISSUER=https://auth.example.com
JWT_AUDIENCE=my-api
JWT_ED25519_ACTIVE_KID=kid-ed-2025-10
JWT_ED25519_PRIVATE_KEY_FILE=/secrets/ed25519.key
JWT_ED25519_PUBLIC_KEYS=kid-ed-2025-06=/secrets/old.pub
PUBLIC_BASE_URL=https://auth.example.com
HTTP_ADDR=:8080
